<div class="amenity-form">
  <%= simple_form_for( [  spot, amenity ] ) do |f| %>
    <%= f.input :category %>
    <%= f.input :address %>
    <%= f.input :descripton %>
    <%= f.input :photos, as: :file, input_html: { multiple: true } %>
    <%= f.button :submit, class: "btn btn-danger" %>
  <% end %>
</div>